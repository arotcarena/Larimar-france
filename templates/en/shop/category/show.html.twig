{% extends "en/base.html.twig" %}

{% block title %}{{ category.name }}{% endblock %}

{% block metaTitle %}
    <meta property="og:title" content="{{ category.name }}">
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {{ encore_entry_script_tags('enProductIndex') }}
{% endblock %}

{% block style %}
    {{ parent() }}
    {{ encore_entry_link_tags('enProductIndex') }}
{% endblock %}



{% block body %}
    <div class="breadcrumb">
        <a href={{ path('en_home') }} class="breadcrumb-home-link">Home</a>
        >
        <span class="breadcrumb-item">{{ category.enName }}</span>
    </div>
    
    <h1 class="product-index-static-title">{{ category.enName }}</h1>

    <div id="product-index" data-categoryid="{{ category.id }}">
    </div>
{% endblock %}